{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "experimentalDecorators": true,
    "allowJs": true,
    "jsx": "react",
    "keyofStringsOnly": true,
    "lib": [
      "ES5", "ES2015", "DOM", "ScriptHost", "ES2017", "ESNext.AsyncIterable"
    ],
    "module": "es6",
    "moduleResolution": "node",
    "noStrictGenericChecks": true,
    "outDir": "./dist/",
    "skipLibCheck": true,
    "downlevelIteration": true,
    "paths": {
      "common/*": ["common/*"]
    },
    "sourceMap": true,
    "strict": true,
    "target": "ES5",
    "typeRoots": ["node_modules/@types", "typings"]
  },
  "include": ["./src/**/*"]
}